<!-- Modal -->
<div class="modal fade" id="staticBackdrop" data-backdrop="static" data-keyboard="false" tabindex="-1" role="dialog"
    aria-labelledby="staticBackdropLabel" aria-hidden="true">
        <div class="modal-dialog">
        <div class="modal-content">
            <div class="modal-header">
                <h5 class="modal-title" id="staticBackdropLabel">Make A New Deposit</h5>
                <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                    <span aria-hidden="true">&times;</span>
                </button>
            </div>
            <div class="modal-body">
                <form action="" method="post">
                    <div class="form-group">
                        <label for="">Amount(USD)</label>
                        <input type="number" required name="amount" class="form-control">
                    </div>
                    <div class="form-group">
                        <select class="form-control" name="mode">
                            <option>BTC</option>
                            <option>Ethereum</option>
                            <option>Litecoin</option>
                        </select>
                    </div>
                    <div class="form-group">
                        <button type="submit" name="submit" class="btn btn-info mr-auto"> Proceed</button>
                        <button type="button" class="btn btn-danger ml-auto" data-dismiss="modal">Close</button>
                    </div>
                </form>
            </div>

        </div>
