<script>
function getProfit() {
    var amount = document.getElementById("amount").value * 1;
    var package = document.getElementById("package").value * 1;
    var mins = [1000, 5000, 15000, 25000];
    var maxs = [4999, 14999, 24999, 1000000];
    var percents = [2, 3, 3.7, 4.2];
    var packages = ["Amateur", "Starter", "Basic", "Standard"];
    var min = mins[package];
    var max = maxs[package];

    if (amount >= min && amount <= max) {
        document.getElementById("profit").innerHTML =
            `Expected profit after 4 days is: <strong class="text-success">$` + (((percents[package] /
                    100) * 30 *
                amount) + amount) + `</strong>`
    } else {
        document.getElementById("profit").innerHTML = "<span class=\'text-danger\'>For " + packages[
                package] +
            " Package, Minimum is: $" + min + " and Maximum is: $" + max + "</span>";
    }


    console.log(amount + " We want to calculate porofit " + min);

    return false;
}
</script>

